<table class="table table-striped scrollable">
	{{#each acList}}
		{{#if Alt }}
			<tr class="flight-info" id="id-{{Id}}" >
				<td style="text-align: center;"><i class="fa fa-fighter-jet {{orient}}"></i></td>
				<td style="text-align: right; width:120px;">{{Alt}}</td>
				<td style="text-align: center; width:120px;">{{CNum}}</td>
			</tr>
		{{/if}}
	{{/each}}
</table>